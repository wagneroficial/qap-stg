{
  "version": 3,
  "name": "currentweather",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "endpoints": [
    {
      "endpoint": "/current-weather",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/v1/current-weather",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://api.meteo-network.com",
            "https://staging-api.meteo-network.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/forecast",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/v1/forecast",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://api.meteo-network.com",
            "https://staging-api.meteo-network.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    },
    {
      "endpoint": "/historical/{date}",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/v1/historical/{date}",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://api.meteo-network.com",
            "https://staging-api.meteo-network.com"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}