{
  "version": 3,
  "name": "1password-connect",
  "output_encoding": "no-op",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "port": 8080,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/1password-connect/vaults/{vaultUuid}/items",
      "method": "POST",
      "output_encoding": "no-op",
      "input_headers": [
        "Authorization",
        "X-User",
        "X-Roles"
      ],
      "extra_config": {
        "auth/validator": {
          "alg": "RS256",
          "jwk_url": "https://idp.konneqt.cloud/realms/qap-dev/protocol/openid-connect/certs",
          "propagate_claims": [
            [
              "sub",
              "X-User"
            ],
            [
              "realm_access.roles",
              "X-Roles"
            ]
          ],
          "propagate_headers": [
            "Authorization"
          ]
        },
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        },
        "proxy": {
          "sequential": true
        },
        "error": {
          "return_status_code": 400,
          "return_body": true
        }
      },
      "backend": [
        {
          "url_pattern": "/vaults/{vaultUuid}/items",
          "encoding": "no-op",
          "sd": "static",
          "method": "POST",
          "cache_ttl": "300s",
          "host": [
            "http://1password.local",
            "http://localhost:8080"
          ],
          "disable_host_sanitize": false,
          "extra_config": {
            "plugin/http-client": {
              "name": "qap-krakend-plugin",
              "gateway_name": "1password-connect",
              "endpoint": "/1password-connect/vaults/{vaultUuid}/items"
            }
          }
        }
      ]
    },
